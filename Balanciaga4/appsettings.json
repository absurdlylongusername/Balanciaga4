{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoadBalancer": {
    "ListenEndpoint": "0.0.0.0:8080",
    "Policy": "RoundRobin",
    "BackendEndpoints": [
      "127.0.0.1:9001",
      "127.0.0.1:9002"
    ],
    "Limits": {
      "MaxConnections": 10000,
      "Backlog": 512
    },
    "Timeouts": {
      "ConnectMs": 2000,
      "IdleMs": 2000
    },
    "HealthCheck": {
      "IntervalMs": 2000,
      "ConnectTimeoutMs": 500
    }
  }
}
